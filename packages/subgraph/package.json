{
  "name": "@cowprotocol/sdk-subgraph",
  "version": "1.0.0",
  "description": "CowProtocol subgraph",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --json --outputFile=jest.results.json && npx coveralls < ./coverage/lcov.info",
    "test:coverage:html": "jest --silent=false --coverage --coverageReporters html",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "graphql:codegen": "graphql-codegen --config graphql-codegen.yml",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@cow-sdk/typescript-config": "workspace:*",
    "@graphql-codegen/cli": "5.0.6",
    "@graphql-codegen/typescript": "4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.17.31",
    "coveralls": "^3.1.1",
    "ts-node": "^10.8.2",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3"
  },
  "dependencies": {
    "@cowprotocol/sdk-common": "workspace:*",
    "@cowprotocol/sdk-config": "workspace:*",
    "graphql": "^16.11.0",
    "graphql-request": "^4.3.0"
  }
}
