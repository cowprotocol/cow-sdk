{
  "name": "@cowprotocol/sdk-common",
  "version": "0.3.0",
  "description": "Common utilities and types for CoW Protocol SDK",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@cow-sdk/typescript-config": "workspace:*",
    "@types/node": "^24.10.1",
    "@types/jest": "^30.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5"
  }
}
