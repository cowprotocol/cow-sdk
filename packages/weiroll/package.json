{
  "name": "@cowprotocol/sdk-weiroll",
  "version": "0.1.0",
  "description": "CowProtocol Weiroll package",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "lint": "eslint src/**/*.ts --quiet",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@cowprotocol/sdk-common": "workspace:*",
    "@cowprotocol/sdk-config": "workspace:*"
  },
  "devDependencies": {
    "@cow-sdk/typescript-config": "workspace:*",
    "@cowprotocol/sdk-ethers-v5-adapter": "workspace:*",
    "@cowprotocol/sdk-ethers-v6-adapter": "workspace:*",
    "@cowprotocol/sdk-viem-adapter": "workspace:*",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@types/semver-sort": "^0.0.5",
    "@ethersproject/constants": "^5.7.0",
    "ethers-v5": "npm:ethers@^5.7.2",
    "ethers-v6": "npm:ethers@^6.13.7",
    "viem": "^2.28.4",
    "jest": "^30.1.3",
    "jest-fetch-mock": "^3.0.3",
    "ts-jest": "^29.4.4",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
