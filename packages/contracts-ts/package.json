{
  "name": "@cowprotocol/sdk-contracts-ts",
  "version": "0.5.0",
  "description": "TypeScript contract bindings for CoW Protocol SDK",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "prebuild": "node scripts/version.js",
    "predev": "node scripts/version.js",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@cowprotocol/sdk-common": "workspace:*",
    "@cowprotocol/sdk-config": "workspace:*"
  },
  "devDependencies": {
    "@cow-sdk/typescript-config": "workspace:*",
    "@cowprotocol/sdk-ethers-v5-adapter": "workspace:*",
    "@cowprotocol/sdk-ethers-v6-adapter": "workspace:*",
    "@cowprotocol/sdk-viem-adapter": "workspace:*",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "ethers-v5": "npm:ethers@^5.7.2",
    "ethers-v6": "npm:ethers@^6.13.7",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "viem": "^2.28.4"
  }
}
